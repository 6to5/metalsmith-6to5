{
  "name": "metalsmith-babel",
  "version": "1.0.0",
  "description": "babel plugin for Metalsmith",
  "repository": "babel/metalsmith-babel",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "jscs *.js && eslint *.js",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/babel/metalsmith-babel/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js"
  ],
  "keywords": [
    "metalsmith",
    "metalsmith-plugin",
    "babel",
    "ecmascript",
    "script",
    "transpile",
    "transpiler",
    "language",
    "es6",
    "plugin",
    "harmony"
  ],
  "dependencies": {
    "babel-core": "^4.0.1",
    "object-assign": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^0.14.1",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.11.3",
    "metalsmith": "^1.3.0",
    "tap-spec": "^2.2.1",
    "tape": "^3.5.0"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
