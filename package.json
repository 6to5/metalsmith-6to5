    "babel",
    "ecmascript",
    "script",
    "transpile",
    "transpiler",
    "language",
    "es6",
    "plugin",
    "harmony"
  ],
  "dependencies": {
    "babel-core": "^5.4.7",
    "object-assign": "^3.0.0"
  },
  "devDependencies": {
    "@shinnn/eslintrc-node": "^1.0.2",
    "eslint": "^0.22.1",
    "istanbul": "^0.3.14",
    "istanbul-coveralls": "^1.0.2",
    "metalsmith": "^1.7.0",
    "tap-spec": "^4.0.0",
    "tape": "^4.0.0"
  }
