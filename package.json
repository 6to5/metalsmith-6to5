{
  "name": "metalsmith-babel",
  "version": "3.0.0",
  "description": "babel plugin for Metalsmith",
  "repository": "babel/metalsmith-babel",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "scripts": {
    "pretest": "eslint --config @shinnn/node --rule 'no-underscore-dangle: 0' index.js test.js",
    "test": "node --strong_mode test.js | tap-spec",
    "coverage": "node --strong_mode node_modules/.bin/istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "keywords": [
    "metalsmith",
    "metalsmith-plugin",
    "babel",
    "ecmascript",
    "script",
    "transpile",
    "transpiler",
    "language",
    "es6",
    "plugin",
    "harmony"
  ],
  "dependencies": {
    "babel-core": "^5.8.24",
    "slash": "^1.0.0"
  },
  "devDependencies": {
    "@shinnn/eslint-config-node": "^1.0.1",
    "eslint": "^1.5.1",
    "istanbul": "^0.3.21",
    "istanbul-coveralls": "^1.0.3",
    "metalsmith": "^2.0.1",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.0"
  }
}
